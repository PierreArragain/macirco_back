{
  "name": "macirco_back",
  "version": "0.0.1",
  "description": "",
  "author": "Pierre Arragain-Dehlinger",
  "private": true,
  "license": "GNU GPLv3",
  "scripts": {
    "prebuild": "node_modules/.bin/rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=local nest start --watch",
    "start:dev": "NODE_ENV=development nest start",
    "start:staging": "NODE_ENV=staging nest start",
    "start:prod": "NODE_ENV=production nest start",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node ./node_modules/typeorm/cli migration:run -d ./src/common/config/database/migration.config.ts",
    "typeorm:run-migrations": "yarn run typeorm migration:run -- -d ./src/common/config/database/migration.config.ts",
    "typeorm:generate-migration": "ts-node ./node_modules/typeorm/cli migration:generate ./src/common/migrations -d ./src/common/config/database/migration.config.ts",
    "typeorm:create-migration": "ts-node ./node_modules/typeorm/cli migration:create -d ./src/common/config/database/migration.config.ts",
    "typeorm:revert-migration": "ts-node ./node_modules/typeorm/cli migration:revert -d ./src/common/config/database/migration.config.ts"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^7.1.6",
    "@nestjs/terminus": "^10.0.1",
    "@nestjs/typeorm": "^10.0.0",
    "axios": "^1.4.0",
    "dotenv": "^16.3.1",
    "helmet": "^7.0.0",
    "pg": "^8.11.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rss-parser": "^3.13.0",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/node": "^20.4.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
